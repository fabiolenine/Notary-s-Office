<!DOCTYPE html>
<html lang="pt-br" ng-app="noApp">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Notarys Office</title>

    <!-- Bootstrap -->
    <link href="../vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="../vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  </head>

	<style>
	#ytplayer{
		position: fixed;
		left: 0px;
		top: 0px;
		z-index: -1;}
	
	div{
		position: absolute;
		background-color: red;
		left: 71%;
		top: 1%;
		width: 28%;
		height: 98%;
		opacity: 0.9;
		z-index: 1;
		}
		
	.cabecalho{	
		position: relative;
		color: white;
		font-size: 56px;
		left: 2%;
		top: 1%;
		z-index: 2}
		
	.conteudo {	
		position: relative;
		color: white;
		font-size: 56px;
		z-index: 2}
	</style>
	
  <body class="nav-md" data-ng-controller="sequenceControllerPainel">
	  <div class="quadro"> 
		<table class="">
			<thead>
				<tr class="cabecalho">
					<th class="">Senha</th>
                    <th class="">Box</th>
           	</tr>
			</thead>
            <tbody>
				<tr class="conteudo" data-ng-repeat="registro in sequencechamadas track by $index">
					<td class="">{{registro.sequence}}</td>
					<td class="">{{registro.guiche}}</td>
                </tr>
			</tbody>
		  </table>
	  </div>

			  <iframe id		= "ytplayer" 
				  type			= "text/html" 
				  width			= "100%" 
				  height		= "100%"
				  src			= "https://www.youtube.com/embed/videoseries?list=PLMC9KNkIncKtPzgY-5rmhvj7fax8fdxoj&autoplay=1&controls=0&loop=1&color=white"
				  frameborder	= "0" 
				  allowfullscreen>
				  
				  <!-- ?autoplay=1&controls=0&loop=1&color=white" -->
	  
	<!-- jQuery -->
	<script src="../vendors/jquery/dist/jquery.min.js"></script>
	<!-- Angularjs !.5.8 -->  
	<script src="../js/angularjs/angular.min.js"></script>
	<script src="../js/angularjs/angular-route.min.js"></script>
	<script src="../js/angular-filter/angular-filter.min.js"></script>
	<!-- Bootstrap -->
	<script src="../vendors/bootstrap/dist/js/bootstrap.min.js"></script>
	<!-- Controllers do Notarys Office -->
 	<script src="../modulos/sequenceRouters.js"></script>
	<script src="../modulos/sequenceControllerConfig.js"></script>
	<script src="../modulos/sequenceControllerService.js"></script>
	<script src="../modulos/sequenceControllerLista.js"></script>
	<script src="../modulos/sequenceControllerPainel.js"></script>
	<script src="../modulos/sequenceControllerMain.js"></script>
	<script src="../modulos/sequenceCore.js"></script>
	<!-- Socket.io -->
	<script src="../js/socket.io/socket.io.js"></script>
	  
  </body>
</html>